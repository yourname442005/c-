#include<stdio.h>
int main(){
    int a[]={1,2,3,4,5,6,7,8,9,0};
    int element=9;
    int count=0;
    int start=0;
    int stop= sizeof(a)/sizeof(int);
    while(1){
        int mid= start+((start-stop)/2);
        if(a[mid]==element){
            printf("madhyamgram is reached");
            break;
        }
        else if(a[mid]>element){
            stop=mid-1;
        }
        else{
            stop=mid+1;
            
        }
        count++;
        if(count>stop){
            printf("madhaymggram not reached");
            break;
        }
    }
}
